import java.util.Scanner;

public class ContaDoSegundoGrau {
	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("Informe o valor de a");
		int a = scanner.nextInt(); //scanner é dado a cada interação

		System.out.println("Informe o valor de b");
		int b = scanner.nextInt(); //scanner é dado a cada interação

		System.out.println("Informe o valor de c");
		int c = scanner.nextInt(); //scanner é dado a cada interação
		
		double delta = calculateDelta(a,b,c);

		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}
	
	public static double calculaDelta(int a, int b, int c);
}
